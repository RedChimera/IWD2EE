			READ_SHORT (thepaneloffset + i * 0x1C + 0xE) thenumcontrols
			READ_SHORT (thepaneloffset + i * 0x1C + 0x18) thefirstcontrolindex
			FOR (j = thefirstcontrolindex; j < thefirstcontrolindex + thenumcontrols; ++j) BEGIN
