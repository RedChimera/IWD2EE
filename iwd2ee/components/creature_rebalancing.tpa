<<<<<<<< .../script.baf
IF
	See(NearestEnemyOf(Myself),0)
	Or(2)
		General(Myself,HUMANOID)
		Race(Myself,YUANTI)
	LevelInClassGT(Myself,20,FIGHTER)
	!CheckSpellState(Myself,KAI)
	!CheckSpellState(Myself,EXECUTIONERS_EYES)
	Or(2)
		!CheckStat(Myself,0,AURACLEANSING)
		!GlobalTimerNotExpired("US_Cast","LOCALS")
	!GlobalTimerNotExpired("US_Maximize","LOCALS")
THEN
	RESPONSE #100
		SetGlobalTimer("US_Cast","LOCALS",6)
		SetGlobalTimer("US_Maximize","LOCALS",2400)
		SpellNoDec(Myself,INNATE_MAXIMIZE_ATTACKS)
		WaitAnimation(Myself,WALK)
		WaitAnimation(Myself,CONJURE)
		WaitAnimation(Myself,CAST)
END

IF
	See(NearestEnemyOf(Myself),0)
	LevelInClassGT(Myself,2,MONK)
THEN
	RESPONSE #100
		SelectWeaponAbility(SLOT_FIST,0)
		AttackOneRound(LastMarkedObject)
END

