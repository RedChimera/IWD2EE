// Patches summoned creatures so they will gain HoF bonuses if an enemy summons them on Heart of Fury mode.

COPY_EXISTING_REGEXP ~00s.*\.cre~ ~override~
	READ_BYTE allegiance theallegiance
	PATCH_IF theallegiance <= 30 BEGIN
		WRITE_BYTE allegiance 255
	END
	BUT_ONLY_IF_IT_CHANGES

// Patches all spellcasters to have one pip in all Spell Focus feats. Effectively increases all enemy spells' save DCs by 2, except for casters who already had Spell Focus.

COPY_EXISTING_REGEXP ~.*\.cre~ ~override~
	READ_BYTE levelcleric clericlevel
	READ_BYTE leveldruid druidlevel
	READ_BYTE levelsorcerer sorcererlevel
	READ_BYTE levelwizard wizardlevel
	PATCH_IF clericlevel != 0 OR druidlevel != 0 OR sorcererlevel != 0 OR wizardlevel != 0 BEGIN
		READ_LONG feats2 thefeats2
		thefeats2|=0xF0000000
		WRITE_LONG feats2 thefeats2
		READ_BYTE spellfocusenchantment ulgiash
		PATCH_IF ulgiash < 1 BEGIN
			WRITE_BYTE spellfocusenchantment 1
		END
		READ_BYTE spellfocusevocation ulgiash
		PATCH_IF ulgiash < 1 BEGIN
			WRITE_BYTE spellfocusevocation 1
		END
		READ_BYTE spellfocusnecromancy ulgiash
		PATCH_IF ulgiash < 1 BEGIN
			WRITE_BYTE spellfocusnecromancy 1
		END
		READ_BYTE spellfocustransmutation ulgiash
		PATCH_IF ulgiash < 1 BEGIN
			WRITE_BYTE spellfocustransmutation 1
		END
	END
	BUT_ONLY_IF_IT_CHANGES

// Fixing a bug that sometimes caused non-arcane spellcasters to get arcane spell failure.

COPY_EXISTING_REGEXP ~.*\.cre~ ~override~
	READ_BYTE levelbard bardlevel
	READ_BYTE levelsorcerer sorcererlevel
	READ_BYTE levelwizard wizardlevel
	PATCH_IF bardlevel = 0 AND sorcererlevel = 0 AND wizardlevel = 0 BEGIN
		READ_LONG feats1 thefeats1
		thefeats1|=0x10
		WRITE_LONG feats1 thefeats1
		WRITE_BYTE armoredarcana 20
	END
	BUT_ONLY_IF_IT_CHANGES

// Ogres and trolls should be giant humanoids rather than monsters.

COPY_EXISTING_REGEXP ~.*\.cre~ ~override~
	READ_LONG animation theanimation
	READ_BYTE general thegeneral
	PATCH_IF  thegeneral = 255 AND (theanimation = 36864 OR theanimation = 59160 OR theanimation = 59176 OR theanimation = 59192 OR theanimation = 59368) BEGIN
		WRITE_BYTE general 5
	END
	BUT_ONLY_IF_IT_CHANGES

//Instructing the game to recalculate each creature's base attack bonus rather than using the value in the CRE file.

COPY_EXISTING_REGEXP ~.*\.cre~ ~override~
	WRITE_BYTE baseattackbonus 0
	BUT_ONLY_IF_IT_CHANGES

