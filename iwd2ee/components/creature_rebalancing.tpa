// Patches monks so they use monk fists at 12 levels higher on Heart of Fury Mode (otherwise their fists are treated as if their level wasn't increased)

COPY_EXISTING_REGEXP ~.*\.cre~ ~override~
	READ_BYTE levelmonk monklevel
	PATCH_IF monklevel > 2 BEGIN
		READ_LONG 0x612 itemslotsoffset
		READ_SSHORT (itemslotsoffset + 0x6) theslot
		PATCH_IF theslot = ~-1~ BEGIN
			LPF	IWD2_ADD_CRE_ITEM INT_VAR STR_VAR item=~USCWHFMO~ slot=~gloves~ END

		END
		ELSE BEGIN
			READ_SSHORT (itemslotsoffset + 0x8) theslot
			PATCH_IF theslot = ~-1~ BEGIN
				LPF	IWD2_ADD_CRE_ITEM INT_VAR STR_VAR item=~USCWHFMO~ slot=~lring~ END
			END
			ELSE BEGIN
				READ_SSHORT (itemslotsoffset + 0xA) theslot
				PATCH_IF theslot = ~-1~ BEGIN
					LPF	IWD2_ADD_CRE_ITEM INT_VAR STR_VAR item=~USCWHFMO~ slot=~rring~ END
				END
				ELSE BEGIN
					READ_SSHORT (itemslotsoffset + 0xC) theslot
					PATCH_IF theslot = ~-1~ BEGIN
						LPF	IWD2_ADD_CRE_ITEM INT_VAR STR_VAR item=~USCWHFMO~ slot=~amulet~ END
					END
					ELSE BEGIN
						READ_SSHORT (itemslotsoffset + 0xE) theslot
						PATCH_IF theslot = ~-1~ BEGIN
							LPF	IWD2_ADD_CRE_ITEM INT_VAR STR_VAR item=~USCWHFMO~ slot=~belt~ END
						END
						ELSE BEGIN
							READ_SSHORT (itemslotsoffset + 0x10) theslot
							PATCH_IF theslot = ~-1~ BEGIN
								LPF	IWD2_ADD_CRE_ITEM INT_VAR STR_VAR item=~USCWHFMO~ slot=~boots~ END
							END
							ELSE BEGIN
								READ_SSHORT (itemslotsoffset + 0x2A) theslot
								PATCH_IF theslot = ~-1~ BEGIN
									LPF	IWD2_ADD_CRE_ITEM INT_VAR STR_VAR item=~USCWHFMO~ slot=~cloak~ END
								END
							END
						END
					END
				END
			END
		END
	END
	BUT_ONLY_IF_IT_CHANGES

// Patches summoned creatures so they will gain HoF bonuses if an enemy summons them on Heart of Fury mode.

COPY_EXISTING_REGEXP ~00s.*\.cre~ ~override~
	READ_BYTE allegiance theallegiance
	PATCH_IF theallegiance <= 30 BEGIN
		WRITE_BYTE allegiance 255
	END
	BUT_ONLY_IF_IT_CHANGES

// Patches all spellcasters to have two pips in all Spell Focus feats. Effectively increases all enemy spells' save DCs by 4, except for casters who already had Spell Focus.

COPY_EXISTING_REGEXP ~.*\.cre~ ~override~
	READ_BYTE levelcleric clericlevel
	READ_BYTE leveldruid druidlevel
	READ_BYTE levelsorcerer sorcererlevel
	READ_BYTE levelwizard wizardlevel
	PATCH_IF clericlevel != 0 OR druidlevel != 0 OR sorcererlevel != 0 OR wizardlevel != 0 BEGIN
		READ_LONG feats2 thefeats2
		thefeats2|=0xF0000000
		WRITE_LONG feats2 thefeats2
		WRITE_BYTE spellfocusenchantment 2
		WRITE_BYTE spellfocusevocation 2
		WRITE_BYTE spellfocusnecromancy 2
		WRITE_BYTE spellfocustransmutation 2
	END
	BUT_ONLY_IF_IT_CHANGES

